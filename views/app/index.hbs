<h2 style="text-align: center;color:rgb(216, 228, 238);">Please Select Your Card</h2>


{{#each user}}
<div class="row" style="text-align: center; font-weight: bold;">
    <div class="col s12 m6">
        <div class="card blue-grey darken-1">
            <div class="card-content white-text">
                <a href={{ _id }}><span class="card-title"> {{ name }} </span></a>
                <p>{{ weight }}</p>
            </div>
        </div>
    </div>
</div>


{{/each}}